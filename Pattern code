
#include <iostream>

using namespace std;

int main()
{
   //i for rows
    int i=0;
    //n is no of rows 
    int n;
    std::cin >> n ;
    while(i<n)
    {
        //j for columns
        int j=0;
        //Here we will increase or decrease the n value based on columns
        while(j<n-1)
        {
           cout << "*" ; 
           j++;
        }
        
        cout<<endl;
        
        i++;
    }
}
output:
3
**
**
**
int main()
{
    int i=0;
    int n;
    std::cin >> n ;
    while(i<n)
    {
        int j=0;
        while(j<n)
        {
           cout << i ; 
          j++;
        }
         
        
        cout<<endl;
        
        i++;
    }
}
